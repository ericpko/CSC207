      <Label layoutX="228.0" layoutY="267.0" prefHeight="44.0" prefWidth="147.0" text="User Email" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="410.0" layoutY="276.0" prefHeight="30.0" prefWidth="297.0" />
      <PasswordField fx:id="password" layoutX="410.0" layoutY="350.0" prefHeight="30.0" prefWidth="297.0" />
      <Label layoutX="228.0" layoutY="343.0" prefHeight="44.0" prefWidth="147.0" text="Password" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="createAccount" layoutX="228.0" layoutY="474.0" mnemonicParsing="false" onAction="#handleCreateNewAccount" prefHeight="44.0" prefWidth="228.0" text="Create User Account" />
