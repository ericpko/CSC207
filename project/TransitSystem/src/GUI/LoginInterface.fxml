      <Button layoutX="193.0" layoutY="23.0" mnemonicParsing="false" onAction="#onGoToTap" prefHeight="30.0" prefWidth="96.0" text="Go to Tap" />
      <Button layoutX="42.0" layoutY="23.0" mnemonicParsing="false" onAction="#saveAndExit" prefHeight="30.0" prefWidth="96.0" text="Safely Exit" />
      <Button layoutX="859.0" layoutY="23.0" mnemonicParsing="false" onAction="#onRoutePlanner" prefHeight="30.0" prefWidth="120.0" text="Route Planner" />
   </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>
</AnchorPane>
